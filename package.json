{
  "name": "sway",
  "version": "3.0.0-wip",
  "description": "A library that simplifies OpenAPI integrations.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "run-s test-node test-browser",
    "test-node": "nyc mocha test/**/test-*.js",
    "test-browser": "karma start test/browser/karma.conf.js",
    "lint": "eslint .",
    "docs": "jsdoc2md -d output ./index.js lib/typedefs.js lib/types/*.js > docs/API.md",
    "dist": "webpack -c webpack.config.js",
    "build": "run-s lint test docs dist",
    "pipeline": "run-s lint test"
  },
  "keywords": [
    "api",
    "openapi",
    "swagger"
  ],
  "author": {
    "name": "Daniel Golub",
    "email": "danielg6666@gmail.com",
    "url": "https://github.com/danielgolub"
  },
  "bugs": {
    "url": "https://github.com/danielgolub/sway-fork/issues"
  },
  "homepage": "https://github.com/danielgolub/sway-fork",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/danielgolub/sway-fork.git"
  },
  "files": [
    "dist",
    "index.d.ts",
    "index.js",
    "lib"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "babel-loader": "^9.1.2",
    "brfs": "^2.0.2",
    "connect": "^3.7.0",
    "eslint": "^8.40.0",
    "jsdoc-to-markdown": "^8.0.0",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.2.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "transform-loader": "^0.2.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "js-base64": "^3.7.5",
    "js-yaml": "^4.1.0",
    "json-refs": "^3.0.15",
    "json-schema-faker": "^0.5.0-rcv.46",
    "lodash": "^4.17.21",
    "native-promise-only": "^0.8.1",
    "path-to-regexp": "^1.7.0",
